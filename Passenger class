package Groupproject;

public class Passenger {
	private String name;
	private int age;
	private String gender;
	Passenger(String name, int age, String gender){
		this.name=name;
		this.age=age;
		this.gender=gender;
	}
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	public String getGender() {
		return gender;
	}
	public void showPassenger() {
		System.out.println("Passenger-name: "+name+", Passenger-age: "+age+", Passenger-gender: "+gender);
	}

}

